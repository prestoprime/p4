			<jaxb:package name="eu.prestoprime.model.oaipmh"/>
		</jaxb:schemaBindings>
	</jaxb:bindings>

	<!-- OAI-PMH oai-identifier Schema Bindings -->
	<jaxb:bindings schemaLocation="identifier-patched.xsd" node="/xs:schema">
		<!-- Creates the classes into a custom package -->
		<jaxb:schemaBindings>
			<jaxb:package name="eu.prestoprime.model.oaipmh"/>
		</jaxb:schemaBindings>
	</jaxb:bindings>

	<!-- OAI-PMH Identify Schema Bindings -->
